{
  "compilerOptions": {
    "target": "ES6",
    "allowJs": true,
    "noEmit": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "jsx": "react"
  },
  "include": ["*.ts"]
}
